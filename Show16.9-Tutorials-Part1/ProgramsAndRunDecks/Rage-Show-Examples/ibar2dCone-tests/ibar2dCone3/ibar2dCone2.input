!     SAGE Input - SLAB-GEOMETRY

pname = "ibar"

use_bio = .false.

mincellpe = 10

kread =   -1              ! (start cycle  -1=not a restart)

ncmax =   16000           ! Maximum number of cycles (20000 for fine zones)

numlev = 1             ! start HE detonation at level 12 (later 11 & 10)

tmax = 5.0e-6          ! Maximum time (sec) (full + pulse at 20m)

secmax = 350000           ! Maximum wallclock duration (sec) (350000 = 97.2 hr)

ncedit = 50000              ! frequency of full output and restart dumps
modcyc = 50000              ! frequency of short output

secdump = 1000000         ! wall time period of restart dumps (default 1800 sec)

! -- Identify small regions


tedit = 1.0e-7

debug = 0
tstab = 0.2
cstab = 0.9

! -- Physics packages

dohydro = .true.
dorad   = .false.

! -- Plots
! ---- One dimensional plots

! onenum = 4
! onetype(1) = "rho", "prs", "xdt", "ubx"

grid = .false.

flggeo = 0
flgvec = 0

! Geometry
sphere = .false.          ! 1-D spherical geometry
cylin  = .false.           ! 2-D cylindrical geometry

imxset = 18               ! even number of base grid zones in x direction
jmxset = 18             ! even number of base grid zones in y direction

dxset = .32             ! x zone width 
dyset = .32              ! y zone width 

xzero =  0.0             ! left edge of grid
yzero =  0.00            ! bottom edge of grid

mxcells = 1500000          ! maximum number of zones

numdzn = 0                 ! number of dezone regions

! -- EOS setup

nummat = 2                ! number of materials

keos   = -1

matdef(16,1) = 0.4            ! air
matdef(30,1) = 1.0e11

matdef(3,2)   = 1.3e12         ! Cu
matdef(16,2)  = 0.8939
matdef(21,2)  = 8.93
matdef(26,2)  = 0.0257
matdef(28,2)  = 1.0e6
matdef(30,2)  = 2.886e11

matdef(5,2)  =    -1.0e33
matdef(7,2)  = 4.33333e11
matdef(12,2)  =    4.00e9
matdef(39,2)  =       1
matdef(49,2)  =    1.0e10


maxcellpe = 1000000

!  Definition of the material regions

numreg = 3

matreg(1) = 1             ! ambient air
typreg(1) = 0             ! 
prsreg(1) = 1.013e6       ! pressure dy/cm**2
tevreg(1) = 0.0256917     ! # temperature (eV)  (eV*11604.9 = deg K)


matreg(2) = 2             ! Copper
typreg(2) = 6             ! 
prsreg(2) = 1.013e6      ! ambient pressure
tevreg(2) = 0.0256917    ! # 0.024832=15 C  0.0256917=25 C)
ydreg(2) = -16051.3239
xdreg(2) = -16051.3239

xreg(1,2) = 2.88
yreg(1,2) = 2.88
diareg(1,2) = .64  ! diameter not radius

xreg(2,2) = 5.171
yreg(2,2) = 5.171
diareg(2,2) = .64  ! diameter not radius


matreg(3) = 2             ! Copper
typreg(3) = 6             ! 
prsreg(3) = 1.013e6      ! ambient pressure
tevreg(3) = 0.0256917    ! # 0.024832=15 C  0.0256917=25 C)
ydreg(3) =  16051.3239
xdreg(3) =  16051.3239

xreg(1,3) = .589
yreg(1,3) = .589
diareg(1,3) = .64  ! diameter not radius

xreg(2,3) = 2.88
yreg(2,3) = 2.88
diareg(2,3) = .64  ! diameter not radius

! CHANGES

uselast = .false.
numinit = 0
numlev = 1
fvolpct = 0.01

sizemat(1) = 0.32, 0.002
sizebnd(1) = 0.32, 0.002

sizemat(1) = 0.32, 0.02
sizebnd(1) = 0.32, 0.02

sizemat(1) = 0.32, 0.01
sizebnd(1) = 0.32, 0.01


dodmpxdt = .true.

strength_option = 1

hydro_version = 2

interface_option = 2






